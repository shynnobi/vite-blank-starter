{
  "name": "vite-powerflow",
  "private": true,
  "version": "1.1.0",
  "description": "A React + Vite starter, fully containerized for reproducible and collaborative development, with strict code quality tooling and AI pair programming workflow (Cursor rules). Includes comprehensive testing, linting, and CI/CD configurations following industry best practices.",
  "author": {
    "name": "Shynn",
    "url": "http://github.com/shynnobi"
  },
  "repository": {
    "type": "git",
    "url": "git+http://github.com/shynnobi/vite-powerflow.git"
  },
  "homepage": "http://github.com/shynnobi/vite-powerflow#readme",
  "bugs": {
    "url": "http://github.com/shynnobi/vite-powerflow/issues"
  },
  "license": "MIT",
  "keywords": [
    "vite",
    "react",
    "typescript",
    "tailwindcss",
    "shadcn-ui",
    "starter-template",
    "zustand",
    "tanstack-query",
    "vitest",
    "playwright",
    "storybook",
    "eslint",
    "prettier",
    "development-workflow",
    "devcontainer",
    "husky",
    "testing"
  ],
  "type": "module",
  "packageManager": "pnpm@10.6.5",
  "scripts": {
    "test": "pnpm turbo run test",
    "test:e2e": "pnpm turbo run test:e2e",
    "test:all": "pnpm turbo run test && pnpm turbo run test:e2e",
    "test:coverage": "pnpm turbo run test:coverage",
    "test:coverage:report": "pnpm turbo run test:coverage:report",
    "lint": "pnpm turbo run lint",
    "lint:root": "eslint . --ext ts,tsx,js,jsx",
    "lint:root:fix": "eslint . --ext ts,tsx,js,jsx --fix",
    "lint:fix": "pnpm turbo run lint:fix",
    "format": "pnpm turbo run format",
    "format:check": "pnpm turbo run format:check",
    "format:root": "prettier --check . --ignore-path .prettierignore",
    "format:root:fix": "prettier --write . --ignore-path .prettierignore",
    "fix": "pnpm turbo run fix",
    "validate:static": "pnpm format:root && pnpm turbo run lint && pnpm turbo run format:check",
    "validate:quick": "pnpm format:root && pnpm turbo run lint && pnpm turbo run format:check && pnpm turbo run test",
    "validate:full": "pnpm format:root && pnpm turbo run lint && pnpm turbo run format:check && pnpm turbo run test && pnpm turbo run test:e2e",
    "validate:precommit": "pnpm format:root && pnpm turbo run validate:precommit",
    "prepare": "husky",
    "postinstall": "node -e \"try { require('child_process').execSync('git rev-parse --is-inside-work-tree', {stdio: 'ignore'}); } catch (e) { require('child_process').execSync('git init', {stdio: 'inherit'}); } process.exit(0);\" && git config core.hooksPath .husky && chmod +x .husky/*",
    "sync-root-to-starter": "tsx packages/tools/cli-scripts/sync-root-to-starter.mts",
    "sync-starter-to-cli": "tsx packages/tools/cli-scripts/sync-starter-to-template.mts",
    "sync-all": "pnpm sync-root-to-starter && pnpm sync-starter-to-cli",
    "cli:build": "pnpm --filter create-powerflow-app build",
    "cli:run": "node packages/cli/dist/index.js",
    "type-check:root": "tsc --noEmit --project tsconfig.json"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@chromatic-com/storybook": "^4.0.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.53.2",
    "@storybook/addon-a11y": "9.1.0-alpha.6",
    "@storybook/addon-docs": "9.1.0-alpha.6",
    "@storybook/addon-themes": "9.1.0-alpha.6",
    "@storybook/addon-vitest": "9.1.0-alpha.6",
    "@storybook/react-vite": "9.1.0-alpha.6",
    "@swc/core": "^1.12.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dir-compare": "^5.0.0",
    "eslint": "^9.30.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "9.1.0-alpha.6",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "listr2": "^8.3.3",
    "msw": "^2.10.3",
    "msw-storybook-addon": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.53.2",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "storybook": "9.1.0-alpha.6",
    "tmp-promise": "^3.0.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "vite": "^7.0.2",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@swc/core",
      "esbuild",
      "msw"
    ],
    "overrides": {
      "rollup": "npm:@rollup/wasm-node",
      "esbuild": "npm:esbuild-wasm@latest"
    }
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@rollup/rollup-darwin-arm64": "^4.44.2",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "find-up": "^7.0.0",
    "glob": "^11.0.3",
    "lucide-react": "^0.525.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.3",
    "rollup": "npm:@rollup/wasm-node@^4.44.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5",
    "zustand": "^5.0.6"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
