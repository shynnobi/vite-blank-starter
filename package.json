{
  "name": "vite-powerflow",
  "private": true,
  "version": "1.1.0",
  "description": "A React + Vite starter, fully containerized for reproducible and collaborative development, with strict code quality tooling and AI pair programming workflow (Cursor rules). Includes comprehensive testing, linting, and CI/CD configurations following industry best practices.",
  "author": {
    "name": "Shynn",
    "url": "http://github.com/shynnobi"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shynnobi/vite-powerflow.git"
  },
  "homepage": "http://github.com/shynnobi/vite-powerflow#readme",
  "bugs": {
    "url": "http://github.com/shynnobi/vite-powerflow/issues"
  },
  "license": "MIT",
  "keywords": [
    "vite",
    "react",
    "typescript",
    "tailwindcss",
    "shadcn-ui",
    "starter-template",
    "zustand",
    "tanstack-query",
    "vitest",
    "playwright",
    "storybook",
    "eslint",
    "prettier",
    "development-workflow",
    "devcontainer",
    "husky",
    "testing"
  ],
  "type": "module",
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "scripts": {
    "generate:aliases": "node scripts/generate-vite-aliases.js",
    "build": "npx turbo run build",
    "test": "npx turbo run test",
    "test:e2e": "npx turbo run test:e2e",
    "test:all": "npx turbo run test && npx turbo run test:e2e",
    "test:coverage": "npx turbo run test:coverage",
    "test:coverage:report": "npx turbo run test:coverage:report",
    "lint": "npx turbo run lint",
    "lint:fix": "npx turbo run lint:fix",
    "format": "npx turbo run format",
    "format:fix": "npx turbo run format:fix",
    "fix": "npx turbo run fix",
    "type-check": "npx turbo run type-check",
    "validate:static": "npx turbo run lint && npx turbo run format && npx turbo run type-check",
    "validate:quick": "npx turbo run lint && npx turbo run format && npx turbo run test",
    "validate:full": "npx turbo run lint && npx turbo run format && npx turbo run test && npx turbo run test:e2e",
    "validate:commit": "npx lint-staged --config .lintstagedrc.cjs && pnpm test",
    "prepare": "husky",
    "postinstall": "./scripts/postinstall.sh",
    "cli:build": "pnpm --filter @vite-powerflow/create build",
    "cli:start": "node packages/cli/dist/index.js",
    "cli:buista": "pnpm cli:build && node packages/cli/dist/index.js",
    "cli:test": "tsx packages/tools/src/cli-scripts/test-cli.mts",
    "predev": "pnpm gen-aliases",
    "sta:dev": "pnpm -filter @vite-powerflow/starter dev",
    "sta:storybook": "pnpm --filter @vite-powerflow/starter storybook",
    "sync:starter-to-template": "pnpm --filter @vite-powerflow/tools sync:starter-to-template",
    "cli:sbt": "pnpm sync:starter-to-template && pnpm --filter @vite-powerflow/create build && pnpm cli:test",
    "generate:barrels": "tsx scripts/generate-barrels.ts",
    "format:root-scripts": "prettier --check scripts",
    "format:root-scripts:fix": "prettier --write scripts",
    "lint:root-scripts": "eslint scripts --ext .ts",
    "typecheck:root-scripts": "tsc --noEmit --project tsconfig.scripts.json",
    "validate:root-scripts": "run-s format:root-scripts lint:root-scripts typecheck:root-scripts",
    "lintstaged:dry": "HUSKY=0 lint-staged --allow-empty && pnpm test"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "enquirer": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.0"
  },
  "devDependencies": {
    "@awalgawe/esbuild-typescript-paths-plugin": "^1.0.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/js-yaml": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "add": "^2.0.6",
    "chalk": "^5.4.1",
    "dir-compare": "^5.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "listr2": "^8.3.3",
    "ora": "^8.2.0",
    "pnpm": "^10.13.1",
    "prettier": "^3.6.2",
    "tmp-promise": "^3.0.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@swc/core",
      "@tailwindcss/oxide",
      "esbuild",
      "msw"
    ]
  }
}
