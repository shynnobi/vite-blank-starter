{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "generate:aliases": {
      "dependsOn": ["^build"],
      "outputs": ["scripts/generated-aliases/**"]
    },
    "@vite-powerflow/tools#generate:aliases": {
      "outputs": [
        "../../tsconfig.base.json",
        "../../vite.aliases.json",
        "../../tsconfig.json",
        "../../vitest.config.ts"
      ]
    },
    "build": {
      "dependsOn": ["@vite-powerflow/tools#generate:aliases", "^build"],
      "outputs": ["dist/**", "src/**/*.d.ts"]
    },
    "dev": {
      "cache": false
    },
    "lint": {},
    "lint:fix": {},
    "test": {},
    "test:unit": {},
    "test:integration": {},
    "test:e2e": {},
    "test:coverage": {},
    "test:coverage:report": {},
    "format": {},
    "fix": {},
    "validate:precommit": {},
    "clean": {
      "outputs": []
    }
  }
}
